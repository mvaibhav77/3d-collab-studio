{
  "name": "3d-collab-studio",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:migrate": "pnpm --filter=api db:migrate:simple",
    "db:studio": "pnpm --filter=api db:studio",
    "db:reset": "pnpm --filter=api db:reset",
    "db:setup": "pnpm --filter=api db:setup",
    "db:fresh": "pnpm --filter=api db:fresh",
    "db:check": "pnpm --filter=api db:check"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "webpack": "^5.101.0"
  }
}
